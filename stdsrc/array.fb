
//TODO these are currently going to be required to be generated by the compiler
//Tese are just theorectical typings for the functions
extern "intrinsic" let (!!) : &[T;N] -> usize -> &T

extern "intrinsic" let (!^) : &mut [T;N] -> usize -> &mut T

for<T,U,const N:usize> public let map func arr : (T->U) -> [T;N] -> [U;N] = arr.map func

for<T,U,const N : usize> implements [T;N] =
    let map self (func:T->U) = [for t in self do func t];
