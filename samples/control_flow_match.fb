let match_expr_ints x : int32 -> int32 = match x where
    | 1 -> 1,
    | 2 -> 3,
    | _ -> 4,

let match_expr_with_block x : int32 -> int32 = match x where
    | 1 ->
        let a : int32 = 2;
        a*3
    | 2 -> 2,
    | a -> a/2,

let match_statement x : int32 -> () =
    match x where
    | 1 -> 
        foo 0;
    | 2 ->
        bar 1;
    | 3 -> baz 2,

